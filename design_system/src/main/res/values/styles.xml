<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="DSAppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="titleTextColor">@color/ink_5</item>
        <item name="actionMenuTextColor">@color/pri_4</item>
        <item name="actionMenuTextAppearance">@style/ToolbarButtonCTA</item>
        <item name="colorControlHighlight">@color/press_color</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/pri_1</item>
        <item name="colorPrimaryVariant">@color/pri_1</item>
        <item name="colorOnPrimary">@color/white_5</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/pri_1</item>
        <item name="colorSecondaryVariant">@color/pri_1</item>
        <item name="colorOnSecondary">@color/black_5</item>

        <!-- Snackbar color-->
        <item name="snackbarStyle">@style/DSSnackbarStyle</item>
        <item name="snackbarTextViewStyle">@style/DSSnackbarTextStyle</item>
        <item name="snackbarButtonStyle">@style/DSSnackbarButtonStyle</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/bg_01</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <style name="DSSnackbarStyle" parent="Widget.MaterialComponents.Snackbar">
        <item name="android:background">@drawable/shape_snackbar</item>
        <item name="android:layout_margin">@dimen/px16</item>
        <item name="actionTextColorAlpha">1</item>
        <item name="animationMode">slide</item>
    </style>

    <style name="DSSnackbarTextStyle" parent="Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:alpha">1</item>
        <item name="android:textColor">@color/ink_1</item>
        <item name="fontFamily">@font/googlesans_medium</item>
        <item name="android:textAppearance">@style/T16H24R</item>
    </style>

    <style name="DSSnackbarButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Snackbar">
        <item name="android:textColor">@color/pri_1</item>
    </style>

